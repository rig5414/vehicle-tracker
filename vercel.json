{
  "version": 2,
  "buildCommand": "node scripts/vercel-build.js && npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "public": false,
  "ignoreCommand": null,
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_SHARP_PATH": "/tmp/node_modules/sharp"
  }
}
